[messages control]
enable=all
disable=
    fixme,
    global-statement,
    too-few-public-methods,
    no-self-use,
    missing-docstring

[reports]
format=colorized

[basic]
const-rgx=[a-z_]*|[A-Z_]*
docstring-min-length=3
good-names=f,i,ex

[design]
max-attributes = 10

[typecheck]
ignored-classes=_CountingAttr
